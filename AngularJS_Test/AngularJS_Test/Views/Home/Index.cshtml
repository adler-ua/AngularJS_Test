@model dynamic

@{
    Layout = null;
}

<!DOCTYPE html>

<html ng-app="mainApp">
<head>
    <title>title</title>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/app/cardsController.js"></script>
    <script src="~/Scripts/app/app.js"></script>
</head>
<body ng-controller="cardsListController">
<div id="cardsList">
    <div ng-repeat="card in cards" class="cardItem">
        Card {{card.id}}
    </div>
</div>
</body>
</html>